.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.flex{
  display:flex;
  justify-content:center;
  align-items:center;
}


page,view,navigator,text,swiper,swiper-item,rich-text,button,input,image{
  box-sizing: border-box;
}

image{
  width: 100%;
}