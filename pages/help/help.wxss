/* pages/help/help.wxss */
page{
  background:#F8F8F8;
}
.list{
  margin-top:35rpx;
  box-sizing:border-box;
  background:white;
}
.column{
  justify-content: flex-start;
  height:390rpx;
}
.column-left{
  width:120rpx;
  height:390rpx;
  flex-direction:column;
}
.column-left>.left-top{
  font-size:60rpx;
  color:#A32635;
  font-weight:weight;
}
.column-left>.left-middle{
  width:2rpx;
  height:265rpx;
  background:#A32635;
  margin-bottom:5rpx;
  margin-top:15rpx;
}
.column-left>.left-bottom{
  flex-direction:column;
}
.column-left>.left-bottom>image{
  width:30rpx;
  height:15rpx;
}
.column-left>.left-bottom>image:first-child{
  margin-bottom:5rpx;
}
.column-right{
  flex:1;
  height:300rpx;
  flex-direction:column;
  align-items: flex-start;
  justify-content:space-around;
}
.column-right>image{
  width:410rpx;
  height:200rpx;
  background:black;
  margin-top: 20rpx;
}
.column-right>view{
  width: 90%;
  display: flex;
  flex-wrap: wrap;
  color:#666666;
  font-size:24rpx;
}
.circle{
  width: 16rpx;
  height: 16rpx;
  background-color: #A32635;
  border-radius: 50%;
  margin-top: -25rpx;
}