page {
  background-color: #f4f4f4;
}
.tab {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 30;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  width: 100%;
  height: 100rpx;
  box-shadow: 0px 0px 20px 0px rgba(102, 102, 102, 0.1);
}
.tab .tab-left {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab .tab-left .tab-left-one {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.tab .tab-left .tab-left-two {
  width: 70rpx;
  height: 10rpx;
  border-radius: 4rpx;
  background-color: #fff;
  margin-top: 10rpx;
}
.tab .tab-right {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab .tab-right .tab-right-one {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
}
.tab .tab-right .tab-right-two {
  width: 70rpx;
  height: 10rpx;
  border-radius: 4rpx;
  background-color: #fff;
  margin-top: 10rpx;
}
.tab .tab-color {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #A32635;
}
.tab .tab-bgc {
  width: 70rpx;
  height: 10rpx;
  border-radius: 4rpx;
  margin-top: 10rpx;
  background-color: #A32635;
}
.orderUnderway {
  height: 100vh;
  padding: 30 rpx;
  background: #f8f8f8;
}
.orderUnderway-body {
  margin: 0 30rpx;
  background-color: #fff;
  padding: 20rpx 20rpx;
  border-radius: 10px;
}
.orderUnderway-form {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 58rpx;
  padding: 30rpx 0;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 58rpx;
}
.orderUnderway-form .orderUnderway-form-left {
  display: flex;
  align-items: center;
}
.orderUnderway-form .orderUnderway-form-left .orderUnderway-form-circle {
  width: 24rpx;
  height: 24rpx;
  border: 2rpx solid #333333;
  border-radius: 50%;
  margin-right: 24rpx;
}
.orderUnderway-form .orderUnderway-form-left .borderColor {
  background-color: #a32635;
  border-color: #a32635;
}
.orderUnderway-form .orderUnderway-form-left .fontColor {
  color: #a32635;
}
.orderUnderway-form .orderUnderway-form-right {
  width: 24rpx;
  height: 24rpx;
  transform: rotate(225deg);
  border-top: 2rpx solid #333333;
  border-left: 2rpx solid #333333;
}
.orderUnderway-form .arrowsColor {
  border-color: #a32635;
  transform: rotate(45deg);
}
.orderUnderway-details .left {
  display: flex;
  height: 28rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  line-height: 28rpx;
  margin-bottom: 30rpx;
}
.orderUnderway-details .left view:first-child {
  width: 150rpx;
}
.orderUnderway-details .left:first-child {
  margin-top: 30rpx;
}
.orderUnderway-data .people-number {
  display: flex;
  align-items: center;
  padding: 15rpx 0;
}
.orderUnderway-data .people-number .people-number-title {
  height: 26rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  line-height: 26rpx;
  margin: 0 32rpx 0 8rpx;
}
.orderUnderway-data .people-number .people-number-input {
  width: 463rpx;
  height: 60rpx;
  padding: 0 0 0 10rpx;
  background: #ffffff;
  border: 1px solid #cccccc;
  border-radius: 6px;
}
.orderUnderway-data .table-number {
  display: flex;
  align-items: center;
  padding: 15rpx 0;
}
.orderUnderway-data .table-number .table-number-title {
  height: 26rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  line-height: 26rpx;
  margin: 0 32rpx 0 8rpx;
}
.orderUnderway-data .table-number .table-number-input {
  width: 463rpx;
  height: 60rpx;
  padding: 0 0 0 10rpx;
  background: #ffffff;
  border: 1px solid #cccccc;
  border-radius: 6px;
}
.orderUnderway-data .img {
  display: flex;
  align-items: center;
  padding: 15rpx 0;
}
.orderUnderway-data .img .img-title {
  height: 26rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  line-height: 26rpx;
  margin: 0 32rpx 0 8rpx;
}
.orderUnderway-data .img-imageArr {
  display: flex;
  flex-wrap: wrap;
}
.orderUnderway-data .img-image {
  width: 288rpx;
  height: 162rpx;
}
.orderUnderway-data .img-image-box {
  position: relative;
  width: 288rpx;
  height: 162rpx;
  margin: 15rpx;
}
.orderUnderway-data .img-image-box .img-image-del {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
  width: 40rpx;
  height: 40rpx;
  background: #ffffff;
  opacity: 0.3;
}
.orderUnderway-data .img-image-box .img-image-del image {
  width: 20rpx;
  height: 20rpx;
}
.orderUnderway-data .img-uploading {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  width: 288rpx;
  height: 162rpx;
  margin: 15rpx;
  background: #ffffff;
  border: 1px solid #cccccc;
}
.orderUnderway-data .img-uploading image {
  width: 36rpx;
  height: 36rpx;
}
.orderUnderway-data .remark {
  display: flex;
  padding: 15rpx 0;
}
.orderUnderway-data .remark .remark-title {
  height: 44rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  line-height: 44rpx;
  margin-right: 30rpx;
}
.orderUnderway-data .remark .remark-text {
  width: 474rpx;
  height: 260rpx;
  background: #ffffff;
  border: 1rpx solid #cccccc;
  border-radius: 6rpx;
  padding: 17rpx;
}
.orderUnderway-footer {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 110rpx;
  padding: 15rpx 30rpx;
  background: #ffffff;
  box-shadow: 0px 0px 20px 0px rgba(102, 102, 102, 0.1);
}
.orderUnderway-footer view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  background: #eaeaea;
  border-radius: 10rpx;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.orderUnderway-footer .completeColor {
  background: #a32635;
  color: #fff;
}
.showModal {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 20;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
}
.showModal .showModal-body {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 600rpx;
  background: #ffffff;
  border-radius: 20rpx;
  padding-bottom: 104rpx;
}
.showModal .showModal-title {
  height: 34rpx;
  line-height: 34rpx;
  font-size: 34rpx;
  text-align: center;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  margin: 40rpx 0;
}
.showModal .showModal-time,
.showModal .showModal-address,
.showModal .showModal-site {
  width: 100%;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  padding: 0 80rpx 30rpx;
}
.showModal .showModal-footer {
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  width: 100%;
  height: 104rpx;
  border-top: 2rpx solid #eaeaea;
  font-size: 34rpx;
}
.showModal .showModal-footer view {
  flex: 1;
  display: flex;
  justify-content: center;
}
.showModal .showModal-footer view:first-child {
  font-family: PingFang SC;
  color: #999999;
  border-right: 2rpx solid #eaeaea;
}
.showModal .showModal-footer view:last-child {
  font-family: PingFang SC;
  color: #a32635;
}
.canvas-box{
  display: flex;
  align-items: center;
  justify-content: center;
}

